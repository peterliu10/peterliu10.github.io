        <Param val="" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="arrow_list = [arrow1, arrow2, arrow3, arrow4, arrow5, arrow_black];&amp;#10;correct_keys = [key1, key2, key3, key4, key5, &quot;space&quot;];&amp;#10;Choice_Final = 0;&amp;#10;current_arrow = 0;&amp;#10;reaction_times = [];&amp;#10;trial_start_time = globalClock.getTime();&amp;#10;waiting_for_end = false;&amp;#10;arrowStim.image = arrow_list[current_arrow];&amp;#10;continueRoutine = true;&amp;#10;back_color = &quot;blue&quot;;&amp;#10;back_color_num = 0;&amp;#10;Rank_list = [Rank_1, Rank_2, Rank_3];&amp;#10;Block_stage = (Number.parseInt(Block_num) - 1);&amp;#10;console.log(Rank_list[Block_stage]);&amp;#10;Rank = Number.parseInt(Rank_list[Block_stage]);&amp;#10;if ((Rank === 0)) {&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# 读取 5 个箭头和对应的正确按键&amp;#10;arrow_list = [arrow1, arrow2, arrow3, arrow4, arrow5, arrow_black]&amp;#10;correct_keys = [key1, key2, key3, key4, key5, &quot;space&quot;]&amp;#10;Choice_Final = 0&amp;#10;current_arrow = 0  # 当前箭头索引&amp;#10;reaction_times = []  # 记录反应时间&amp;#10;trial_start_time = globalClock.getTime()  # 记录 trial 开始时间&amp;#10;waiting_for_end = False  # 是否进入等待模式&amp;#10;arrowStim.image = arrow_list[current_arrow]  # 显示第一个箭头&amp;#10;continueRoutine = True&amp;#10;back_color = 'blue'&amp;#10;back_color_num = 0&amp;#10;#读取列表&amp;#10;Rank_list = [Rank_1, Rank_2, Rank_3]&amp;#10;Block_stage = int(Block_num)-1&amp;#10;print(Rank_list[Block_stage])&amp;#10;Rank = int(Rank_list[Block_stage])&amp;#10;if Rank == 0 :&amp;#10;    continueRoutine = False" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="# 计算当前试次已经运行的时间&amp;#10;current_time = globalClock.getTime()&amp;#10;elapsed_time = current_time - trial_start_time&amp;#10;&amp;#10;# 监听键盘输入&amp;#10;keys = resp.getKeys()&amp;#10;if keys and not waiting_for_end:&amp;#10;    key = keys[0]  # 获取第一个按键&amp;#10;    rt = keys[0].rt  # 记录反应时间&amp;#10;&amp;#10;    # 检查按键是否正确&amp;#10;    if key.name == correct_keys[current_arrow]:&amp;#10;        reaction_times.append(rt)  # 记录反应时间&amp;#10;        current_arrow += 1  # 进入下一个箭头&amp;#10;        back_color_num += 1&amp;#10;        if back_color_num % 2 == 1:&amp;#10;            back_color = 'green'&amp;#10;        else:&amp;#10;            back_color = 'blue'&amp;#10;&amp;#10;        # 如果 5 个箭头完成，进入等待模式&amp;#10;        if current_arrow &gt;= len(arrow_list)-1:&amp;#10;            waiting_for_end = True  # 进入等待模式&amp;#10;            current_arrow = 5&amp;#10;            waiting_start_time = globalClock.getTime()  # 记录等待时间&amp;#10;            arrowStim.image = &quot;Black.png&quot;  # 隐藏箭头&amp;#10;        else:&amp;#10;            # 继续显示下一个箭头&amp;#10;            arrowStim.image = arrow_list[current_arrow]&amp;#10;&amp;#10;# 如果 4 秒到了，不管有没有完成 5 个箭头，直接终止&amp;#10;if elapsed_time &gt;= 4:&amp;#10;    continueRoutine = False&amp;#10;&amp;#10;# 进入等待模式，保持 Routine 直到 4 秒&amp;#10;if waiting_for_end:&amp;#10;    if globalClock.getTime() - trial_start_time &gt;= 4:&amp;#10;        continueRoutine = False  # 结束 routine&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="current_time = globalClock.getTime();&amp;#10;elapsed_time = (current_time - trial_start_time);&amp;#10;keys = resp.getKeys();&amp;#10;if ((keys &amp;&amp; (! waiting_for_end))) {&amp;#10;    key = keys[0];&amp;#10;    rt = keys[0].rt;&amp;#10;    if ((key.name === correct_keys[current_arrow])) {&amp;#10;        reaction_times.push(rt);&amp;#10;        current_arrow += 1;&amp;#10;        back_color_num += 1;&amp;#10;        if (((back_color_num % 2) === 1)) {&amp;#10;            back_color = &quot;green&quot;;&amp;#10;        } else {&amp;#10;            back_color = &quot;blue&quot;;&amp;#10;        }&amp;#10;        if ((current_arrow &gt;= (arrow_list.length - 1))) {&amp;#10;            waiting_for_end = true;&amp;#10;            current_arrow = 5;&amp;#10;            waiting_start_time = globalClock.getTime();&amp;#10;            arrowStim.image = &quot;Black.png&quot;;&amp;#10;        } else {&amp;#10;            arrowStim.image = arrow_list[current_arrow];&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;if ((elapsed_time &gt;= 4)) {&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;if (waiting_for_end) {&amp;#10;    if (((globalClock.getTime() - trial_start_time) &gt;= 4)) {&amp;#10;        continueRoutine = false;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="psychoJS.experiment.addData(&quot;ReactionTimes&quot;, reaction_times);&amp;#10;psychoJS.experiment.addData(&quot;TotalArrowsReacted&quot;, current_arrow);&amp;#10;psychoJS.experiment.addData(&quot;TrialDuration&quot;, 4);&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# 记录数据&amp;#10;thisExp.addData(&quot;ReactionTimes&quot;, reaction_times)&amp;#10;thisExp.addData(&quot;TotalArrowsReacted&quot;, current_arrow)&amp;#10;thisExp.addData(&quot;TrialDuration&quot;, 4)&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
